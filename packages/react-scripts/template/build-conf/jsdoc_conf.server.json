{
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./docs/out/server"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "server"
    ],
    "includePattern": ".*\\.js$"
  },
  "plugins": [],
  "templates": {
    "theme": "flatly",
    "systemName": "adsum-project-boilerplate-server",
    "copyright": "Copyright ADACTIVE 2016",
    "includeDate": false,
    "collapseSymbols": false,
    "syntaxTheme": "dark",
    "outputSourceFiles": false,
    "outputSourcePath": false,
    "linenums": false
  },
  "markdown": {
    "parser": "gfm"
  }
}
